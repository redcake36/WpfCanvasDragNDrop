  </ItemGroup>

  <ItemGroup>
    <Content Include="element.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Resource Include="Resources\светлая стрелка.png" />
    <Resource Include="Resources\стре.png" />
  </ItemGroup>

</Project>
